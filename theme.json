{
	"name": "IKA.ink",
	"slug": "ikaink",
	"description": "A custom theme for ika.ink build on Fusion CMS",
	"author": "Manda Eldreth",
	"version": "1.0.0",
	"public_theme": true,
	"admin_theme": false,
	"dependencies": {
    "cms": "5.13.*"
	},
	"composers": {
		"index": "HomepageComposer",
		"layouts.default": "ThemeSettingsComposer",
		"components.social-media.tags": "ThemeSettingsComposer",
		"matrix.scrapbook.index": "ScrapbookComposer",
		"matrix.gallery.index": "GalleryComposer",
		"matrix.projects.index": "ProjectsComposer",
		"seo::sitemap": "SitemapComposer"
	},
	"settings": {
		"public_images": "dist/images",
		"site_logo": "dist/images/logo.png",
		"theme_color": "#ffffff",
		"custom_sitemap": false,
		"images": {
			"1x1": "dist/images/1x1.png",
			"default": "dist/images/galaxy.jpg",
			"loading": "dist/images/loading.svg",
			"page-banner": "dist/images/galaxy.jpg"
		},
		"icons": {},
		"pagination": {}
	}
}
